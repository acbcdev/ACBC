---
import Card from "./Card.astro";
import WWW from "@/svg/www.svg";
import { projects } from "./const";
import Title from "../Title.astro";
---

<section class="space-y-6 mb-10 px-4 md:px-10 grid place-content-center">
  <Title>
    <WWW /> Projects
  </Title>
  <section class="grid gap-x-3 gap-y-4 md:grid-cols-2">
    {projects.map((proyect) => <Card {...proyect} />)}
  </section>
</section>
